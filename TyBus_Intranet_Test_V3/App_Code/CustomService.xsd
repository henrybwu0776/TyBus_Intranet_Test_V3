            <MainSource>
              <DbSource ConnectionRef="connERPSQL (Web.config)" DbObjectName="SELECT ISNULL(LinesNo, '無路線別') AS LinesNo, ISNULL(AthorityDepNo, '') AS AthorityDepNo, ISNULL ((SELECT NAME FROM DEPARTMENT WHERE (DEPNO = a.AthorityDepNo)), '不分單位') AS AthorityDepNo_C, ISNULL(ServiceType, '') AS ServiceType, (SELECT TypeText FROM CustomServiceType WHERE (TypeStep = '1') AND (TypeLevel1 = a.ServiceType)) AS ServiceType_C, ISNULL(ServiceTypeB, '') AS ServiceTypeB, (SELECT TypeText FROM CustomServiceType AS CustomServiceType_5 WHERE (TypeStep = '2') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB)) AS ServiceTypeB_C, ISNULL(ServiceTypeC, '') AS ServiceTypeC, (SELECT TypeText FROM CustomServiceType AS CustomServiceType_4 WHERE (TypeStep = '3') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB) AND (TypeLevel3 = a.ServiceTypeC)) AS ServiceTypeC_C, ISNULL(Driver, '') AS Driver FROM CustomService AS a WHERE (1 = 1) UNION ALL SELECT ISNULL(LinesNo2, '無路線別') AS LinesNo, ISNULL(AthorityDepNo2, '') AS AthorityDepNo, ISNULL ((SELECT NAME FROM DEPARTMENT AS DEPARTMENT_1 WHERE (DEPNO = a.AthorityDepNo2)), '不分單位') AS AthorityDepNo_C, ISNULL(ServiceType, '') AS ServiceType, (SELECT TypeText FROM CustomServiceType AS CustomServiceType_3 WHERE (TypeStep = '1') AND (TypeLevel1 = a.ServiceType)) AS ServiceType_C, ISNULL(ServiceTypeB, '') AS ServiceTypeB, (SELECT TypeText FROM CustomServiceType AS CustomServiceType_2 WHERE (TypeStep = '2') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB)) AS ServiceTypeB_C, ISNULL(ServiceTypeC, '') AS ServiceTypeC, (SELECT TypeText FROM CustomServiceType AS CustomServiceType_1 WHERE (TypeStep = '3') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB) AND (TypeLevel3 = a.ServiceTypeC)) AS ServiceTypeC_C, ISNULL(Driver2, '') AS Driver FROM CustomService AS a WHERE (ISNULL(LinesNo2, N'') &lt;&gt; '')" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          ISNULL(LinesNo, '不分路線') AS 路線代號, AthorityDepNo AS 單位編號, AthorityDepNo_C AS 單位, 
                            ServiceType AS 反映事項代號, ServiceType_C AS 反映事項, CASE WHEN isnull(ServiceTypeB, '') 
                            = '' THEN ServiceType ELSE ServiceTypeB END AS 客訴事項代號, CASE WHEN isnull(ServiceTypeB_C, '') 
                            = '' THEN ServiceType_C ELSE ServiceTypeB_C END AS 客訴事項, CASE WHEN (isnull(ServiceTypeC, '') = '') AND 
                            (isnull(ServiceTypeB, '') = '') THEN ServiceType WHEN (isnull(ServiceTypeC, '') = '') AND (isnull(ServiceTypeB, '') 
                            &lt;&gt; '') THEN ServiceTypeB ELSE ServiceTypeC END AS 客訴類別代號, CASE WHEN (isnull(ServiceTypeC_C, '') = '') 
                            AND (isnull(ServiceTypeB_C, '') = '') THEN ServiceType_C WHEN (isnull(ServiceTypeC_C, '') = '') AND 
                            (isnull(ServiceTypeB_C, '') &lt;&gt; '') THEN ServiceTypeB_C ELSE ServiceTypeC_C END AS 客訴類別, 
                            Driver AS 駕駛員代號, ISNULL
                                ((SELECT          NAME
                                    FROM              EMPLOYEE
                                    WHERE          (EMPNO = t.Driver) AND (LEAVEDAY IS NULL)), '不分駕駛員') AS 駕駛員, COUNT(Driver) 
                            AS 筆數
FROM              (SELECT          ISNULL(LinesNo, '無路線別') AS LinesNo, ISNULL(AthorityDepNo, '') AS AthorityDepNo, ISNULL
                                                            ((SELECT          NAME
                                                                FROM              DEPARTMENT
                                                                WHERE          (DEPNO = a.AthorityDepNo)), '不分單位') AS AthorityDepNo_C, ISNULL(ServiceType, '') 
                                                        AS ServiceType,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType
                                                              WHERE           (TypeStep = '1') AND (TypeLevel1 = a.ServiceType)) AS ServiceType_C, 
                                                        ISNULL(ServiceTypeB, '') AS ServiceTypeB,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType AS CustomServiceType_5
                                                              WHERE           (TypeStep = '2') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB)) 
                                                        AS ServiceTypeB_C, ISNULL(ServiceTypeC, '') AS ServiceTypeC,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType AS CustomServiceType_4
                                                              WHERE           (TypeStep = '3') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB) 
                                                                                          AND (TypeLevel3 = a.ServiceTypeC)) AS ServiceTypeC_C, ISNULL(Driver, '') 
                                                        AS Driver
                            FROM               CustomService AS a
                            WHERE           (1 = 1)
                            UNION ALL
                            SELECT          ISNULL(LinesNo2, '無路線別') AS LinesNo, ISNULL(AthorityDepNo2, '') AS AthorityDepNo, 
                                                        ISNULL
                                                            ((SELECT          NAME
                                                                FROM              DEPARTMENT AS DEPARTMENT_1
                                                                WHERE          (DEPNO = a.AthorityDepNo2)), '不分單位') AS AthorityDepNo_C, ISNULL(ServiceType, '') 
                                                        AS ServiceType,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType AS CustomServiceType_3
                                                              WHERE           (TypeStep = '1') AND (TypeLevel1 = a.ServiceType)) AS ServiceType_C, 
                                                        ISNULL(ServiceTypeB, '') AS ServiceTypeB,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType AS CustomServiceType_2
                                                              WHERE           (TypeStep = '2') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB)) 
                                                        AS ServiceTypeB_C, ISNULL(ServiceTypeC, '') AS ServiceTypeC,
                                                            (SELECT          TypeText
                                                              FROM               CustomServiceType AS CustomServiceType_1
                                                              WHERE           (TypeStep = '3') AND (TypeLevel1 = a.ServiceType) AND (TypeLevel2 = a.ServiceTypeB) 
                                                                                          AND (TypeLevel3 = a.ServiceTypeC)) AS ServiceTypeC_C, ISNULL(Driver2, '') 
                                                        AS Driver
                            FROM              CustomService AS a
                            WHERE          (ISNULL(LinesNo2, N'') &lt;&gt; '')) AS t
WHERE          (1 &lt;&gt; 1)
GROUP BY   LinesNo, AthorityDepNo, AthorityDepNo_C, ServiceType, ServiceType_C, ServiceTypeB, ServiceTypeB_C, 
                            ServiceTypeC, ServiceTypeC_C, Driver
ORDER BY   LinesNo, 單位編號, 反映事項代號, ServiceTypeB, ServiceTypeC, 駕駛員代號</CommandText>
                    <Parameters />
                  </DbCommand>
