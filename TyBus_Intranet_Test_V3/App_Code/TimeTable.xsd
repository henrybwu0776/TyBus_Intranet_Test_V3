                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          a.BUDATE, a.DEPNO,
                                (SELECT          NAME
                                  FROM               DEPARTMENT
                                  WHERE           (DEPNO = a.DEPNO)) AS DepName, b.LinesNo,
                                (SELECT          LineName
                                  FROM               Lines
