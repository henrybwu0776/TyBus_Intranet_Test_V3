                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          applyman,
                                (SELECT          NAME
                                  FROM               EMPLOYEE
                                  WHERE           (EMPNO = a.applyman)) AS ApplyName, depno,
                                (SELECT          NAME
                                  FROM               DEPARTMENT
                                  WHERE           (DEPNO = a.depno)) AS DepName, realday, applytype,
