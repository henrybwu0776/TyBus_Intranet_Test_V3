                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT          WorkNo, CAR_ID, BUDATE, BUMAN,
                                (SELECT          NAME
                                  FROM               EMPLOYEE
                                  WHERE           (EMPNO = a.BUMAN)) AS BuMan_C,
                                (SELECT          CompanyNo
